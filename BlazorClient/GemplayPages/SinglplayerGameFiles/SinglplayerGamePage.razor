@page "/SinglplayerGame"
@page "/SinglplayerGame/{GameName}/{LoggedUserName}"
@inherits SinglplayerGamePageBase

<h3>Play Singplayer Game: @SinglplayerGameMethods.ToString(Game)</h3>

@switch(Game)
{
    case SinglplayerGame.BalckJack:
        <BlackJackGame></BlackJackGame>
        break;
    case SinglplayerGame.TicTacToe:
        <TicTacToeGame></TicTacToeGame>
        break;
    default:
        <div> there is no such game</div>
        break;
}